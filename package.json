{
  "name": "you-function-better",
  "version": "1.0.0",
  "description": "Serverless tutorial",
  "main": "src/handlers/painAway.js",
  "scripts": {
    "dev": "sls offline start",
    "test": "jest",
    "deploy": "sls deploy --stage production",
    "destroy": "sls remove --stage production"
  },
  "jest": {
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "source-map-support": "^0.5.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "0.0.22",
    "@types/jest": "^23.3.1",
    "@types/node": "^8.0.57",
    "jest": "^23.4.2",
    "serverless-offline": "^3.25.7",
    "serverless-webpack": "^4.0.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^2.3.7",
    "typescript": "^2.5.2",
    "webpack": "^3.6.0"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
